{
  "name": "vue-cesium",
  "version": "0.0.10",
  "description": "Load the Cesium official build package or other third-party Cesium packages (such as the Superap WebGL build package) to the Vue component.",
  "main": "index.js",
  "scripts": {
    "build:docs": "webpack -p --config ./build/webpack.docs.config.js --mode production",
    "build": "webpack -p --config ./build/webpack.prod.config.js --mode production",
    "dev": "webpack-dev-server --content-base docs --config ./build/webpack.docs.config.js --hot --inline --mode development",
    "deploy": "npm run build && npm run build:docs"
  },
  "keywords": [
    "Vue",
    "Cesium",
    "SuperMap",
    "WebGL"
  ],
  "author": "zouyaoji <370681295@qq.com> (https://github.com/zouyaoji)",
